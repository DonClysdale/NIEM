<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:epc="http://interoperability.gc.ca/employment/EmploymentProgramCase/2.0/"
	xmlns:niem-xs="http://release.niem.gov/niem/proxy/xsd/4.0/" xmlns:structures="http://release.niem.gov/niem/structures/4.0/" xmlns:nc="http://release.niem.gov/niem/niem-core/4.0/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:hs="http://release.niem.gov/niem/domains/humanServices/4.1/" xmlns:j="http://release.niem.gov/niem/domains/jxdm/6.1/"
	xmlns:em="http://release.niem.gov/niem/domains/emergencyManagement/4.1/" xmlns:tran="http://interoperability.gc.ca/technical/transaction/2.0/"
	xmlns:ip="http://release.niem.gov/niem/domains/infrastructureProtection/4.0/" xmlns:fault="http://interoperability.gc.ca/fault/2.0/"
	targetNamespace="http://interoperability.gc.ca/employment/EmploymentProgramCase/2.0/" elementFormDefault="qualified" version="2.0">
	<xs:annotation>
		<xs:documentation>Elements for employment cases (inspections) for employment related programs.</xs:documentation>
	</xs:annotation>

	<xs:import namespace="http://release.niem.gov/niem/structures/4.0/" schemaLocation="../../../niem/utility/structures/4.0/structures.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/niem-core/4.0/" schemaLocation="../../../niem/niem-core/4.0/niem-core.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/proxy/xsd/4.0/" schemaLocation="../../../niem/proxy/xsd/4.0/xs.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/humanServices/4.1/" schemaLocation="../../../niem/domains/humanServices/4.1/hs.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/infrastructureProtection/4.0/" schemaLocation="../../../niem/domains/infrastructureProtection/4.0/infrastructureProtection.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/jxdm/6.1/" schemaLocation="../../../niem/domains/jxdm/6.1/jxdm.xsd"/>
	<xs:import namespace="http://release.niem.gov/niem/domains/emergencyManagement/4.1/" schemaLocation="../../../niem/domains/emergencyManagement/4.1/emergencyManagement.xsd"/>
	<xs:import namespace="http://interoperability.gc.ca/employment/NationalOccupationClassification/2.0/" schemaLocation="NationalOccupationClassification.xsd"/>
	<xs:import namespace="http://interoperability.gc.ca/technical/transaction/2.0/" schemaLocation="../../technical/2.0/transaction.xsd"/>
	<xs:import namespace="http://interoperability.gc.ca/person/Person/2.0/" schemaLocation="../../person/2.0/Person.xsd"/>

	<xs:element name="ProgramCaseAugmentationPoint" abstract="true"/>
	<xs:complexType name="ProgramCaseType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Program Case</xs:documentation>
			<xs:documentation xml:lang="en">An Program Case is a Case that pertains to one or more Programs.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:CaseType">
				<xs:sequence>
					<xs:element ref="nc:Program" minOccurs="1" maxOccurs="unbounded"/>
					<xs:element ref="epc:ProgramCaseAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ProgramCase" type="epc:ProgramCaseType"/>
	<xs:element name="EmploymentProgramCaseAugmentationPoint" abstract="true"/>
	<xs:complexType name="EmploymentProgramCaseType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Employment Program Case</xs:documentation>
			<xs:documentation xml:lang="en">An Employment Program Case is a Case that pertains to a Person's employment association with an organization within the context of one or more
				Programs.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="epc:ProgramCaseType">
				<xs:sequence>
					<xs:element ref="epc:PersonEmploymentAssociation" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:WorkPermitHolder" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:EmploymentProgramCaseAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="tran:Transaction" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="EmploymentProgramCase" type="epc:EmploymentProgramCaseType"/>
	<xs:element name="EmployeePositionDutiesText" type="nc:TextType"/>
	<xs:element name="EmployeePositionRequiredJobSkillsText" type="nc:TextType">
		<xs:annotation>
			<xs:documentation xml:lang="en">A description of the job skills or experience that the subject requires to perform the job.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PersonResidenceAssociation" type="nc:PersonResidenceAssociationType" substitutionGroup="nc:PersonAugmentationPoint"/>
	<xs:element name="PersonEmploymentFullTimeIndicatorReasonText" type="nc:TextType" substitutionGroup="j:PersonEmploymentUnemploymentStatusAbstract" nillable="true">
		<xs:annotation>
			<xs:documentation>A reason text when employment position is not fulltime.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PersonEmploymentAssociation" type="epc:PersonEmploymentAssociationType"/>
	<xs:element name="PersonEmploymentAssociationAugmentationPoint" abstract="true"/>
	<xs:complexType name="PersonEmploymentAssociationType">
		<xs:annotation>
			<xs:documentation>A data type for additional information about an employment.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="nc:PersonEmploymentAssociationType">
				<xs:sequence>
					<xs:element ref="epc:EmployeePositionDutiesText" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:EmployeePositionRequiredJobSkillsText" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:EmployeePositionEducationLevelRequiredCode" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="epc:EmployeePositionEducationRequiredText" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="em:RegistrationJurisdictionName" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="epc:PersonEmploymentSeasonalIndicator" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="epc:LabourMarketImpactAssementExemption" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="hs:WagesBasePayAmount" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="epc:EmployeeHoursNumericAbstract" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:WagesOvertimePayScale" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:AdditionalEmployeeCompensationAbstract" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:EmployerFundedEmployeeBenefit" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:EmployeeLeaveAbstract" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="epc:EmployerSignature" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="epc:PersonEmploymentAssociationAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="AssociationIdentification" type="nc:IdentificationType" substitutionGroup="nc:AssociationAugmentationPoint"/>
	<xs:element name="AdditionalAssociationIdentification" type="nc:IdentificationType" substitutionGroup="nc:AssociationAugmentationPoint"/>
	<xs:element name="Wages" type="hs:WagesType"/>
	<xs:element name="WorkPermitHolder" type="epc:WorkPermitHolderType"/>
	<xs:complexType name="WorkPermitHolderType">
		<xs:annotation>
			<xs:documentation>A data type for information about the instrument permitting a person to work in a country where they do not permanently reside.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="structures:ObjectType">
				<xs:sequence>
					<xs:element ref="nc:RoleOfPerson" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="nc:Permit" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="nc:Address" minOccurs="0" maxOccurs="1"/>
					<xs:element ref="epc:WorkPermitHolderAugmentationPoint" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="WorkPermitHolderAugmentationPoint" abstract="true">
		<xs:annotation>
			<xs:documentation>An augmentation point for WorkPermitHolderType.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="ReviewIndicator" type="niem-xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">Any review on the work permit holder.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EmployeePositionEducationLevelRequiredCode" type="epc:StudentEducationLevelCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">The level of education that the subject must have attained to perform the job.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EmployeePositionEducationRequiredText" type="nc:TextType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Any specific education required to perform the job.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PersonEmploymentSeasonalIndicator" type="niem-xs:boolean">
		<xs:annotation>
			<xs:documentation xml:lang="en">True if the subject has seasonal employment; false otherwise.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TimePeriodAbstract" abstract="true">
		<xs:annotation>
			<xs:documentation>Time duration concept.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TimePeriodISO8601Text" type="nc:TextType" substitutionGroup="epc:TimePeriodAbstract">
		<xs:annotation>
			<xs:documentation>Time duration conforming to the ISO 8601 syntax for temporal length.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="TimePeriodText" type="nc:TextType" substitutionGroup="epc:TimePeriodAbstract">
		<xs:annotation>
			<xs:documentation>Time duration not conforming to the ISO 8601 syntax for temporal length.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WagesBasePayScaleFactor" type="nc:NonNegativeDecimalType">
		<xs:annotation>
			<xs:documentation>Factor to scale up the WagesBasePayAmount.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WorkedHoursQuantityAbstract" abstract="true">
		<xs:annotation>
			<xs:documentation>Concept for how many hours have been worked in a period.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WorkedHoursPerDayQuantity" type="nc:QuantityType" substitutionGroup="epc:WorkedHoursQuantityAbstract">
		<xs:annotation>
			<xs:documentation>Concept for how many hours have been worked in a day.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="WorkedHoursPerWeekQuantity" type="nc:QuantityType" substitutionGroup="epc:WorkedHoursQuantityAbstract">
		<xs:annotation>
			<xs:documentation>Concept for how many hours have been worked in a week.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="StudentEducationLevelCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type for the classification of the current education level requirements of a non-immigrant alien student.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="05">
				<xs:annotation>
					<xs:documentation>College Level Diploma./Certificate</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="06">
				<xs:annotation>
					<xs:documentation>Bachelor’s degree.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="07">
				<xs:annotation>
					<xs:documentation>Master’s Degree.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="09">
				<xs:annotation>
					<xs:documentation>No Formal Education Requirement.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="24">
				<xs:annotation>
					<xs:documentation>Lower Secondary Education.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>			
			<xs:enumeration value="34">
				<xs:annotation>
					<xs:documentation>Secondary School.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="35">
				<xs:annotation>
					<xs:documentation>Vocational School Diploma/Certificate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="44">
				<xs:annotation>
					<xs:documentation>Apprenticeship Diploma/Certificate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="45">
				<xs:annotation>
					<xs:documentation>Trade Diplomat/Certificate.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="65">
				<xs:annotation>
					<xs:documentation>Bachelor’s or equivalent level, professional.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="84">
				<xs:annotation>
					<xs:documentation>Doctorate/PhD.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="85">
				<xs:annotation>
					<xs:documentation>Doctor of Medicine.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="99">
				<xs:annotation>
					<xs:documentation>Not known or unspecified.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="StudentEducationLevelCodeType">
		<xs:annotation>
			<xs:documentation>A data type for the classification of the current education level requirements of a non-immigrant alien student.</xs:documentation>
			<xs:documentation>The classification is based on International Standard Classification of Education (ISCED) 2011.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="epc:StudentEducationLevelCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="WagesOvertimePayScaleType">
		<xs:annotation>
			<xs:documentation>A data type for scaling the base pay amount (in hours) based on the number of hours worked.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="structures:ObjectType">
				<xs:sequence>
					<xs:element ref="epc:WagesBasePayScaleFactor" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="epc:WorkedHoursQuantityAbstract" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="epc:WagesPayAmountAbstract" minOccurs="1" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="WagesOvertimePayScale" type="epc:WagesOvertimePayScaleType"/>
	<xs:element name="AdditionalEmployeeCompensationAbstract" abstract="true"/>
	<xs:simpleType name="AdditionalEmployeeCompensationCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type for the type of compensation (other than wages/salary) that may be offered to an employee.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="room">
				<xs:annotation>
					<xs:documentation>Room</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="board">
				<xs:annotation>
					<xs:documentation>Board</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="meal">
				<xs:annotation>
					<xs:documentation>Meal allowance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="travel">
				<xs:annotation>
					<xs:documentation>Travel allowance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="vehicle">
				<xs:annotation>
					<xs:documentation>Use of a vehicle</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="leave">
				<xs:annotation>
					<xs:documentation>Paid vacation leave</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="living">
				<xs:annotation>
					<xs:documentation>Living allowance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="bonus">
				<xs:annotation>
					<xs:documentation>Bonus pay</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="other">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AdditionalEmployeeCompensationCodeType">
		<xs:annotation>
			<xs:documentation>A data type for a kind of entity with the obligation.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="epc:AdditionalEmployeeCompensationCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="AdditionalEmployeeCompensationCode" type="epc:AdditionalEmployeeCompensationCodeType" substitutionGroup="epc:AdditionalEmployeeCompensationAbstract">
		<xs:annotation>
			<xs:documentation>Other non-monetaty compensation provided to the employee as part of the employment agreement.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="CompensationCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type for the type of compensation (other than wages/salary) that may be offered to an employee.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation>Monetary</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation>Benefits</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation>Leave</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation>Other</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="CompensationCodeType">
		<xs:annotation>
			<xs:documentation>Other non-monetaty compensation provided to the employee as part of the employment agreement.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="epc:CompensationCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="CompensationCode" type="epc:CompensationCodeType" substitutionGroup="nc:ObligationAugmentationPoint">
		<xs:annotation>
			<xs:documentation>Other non-monetaty compensation provided to the employee as part of the employment agreement.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="AdditionalEmployeeCompensationText" type="nc:TextType" substitutionGroup="epc:AdditionalEmployeeCompensationAbstract"/>
	<xs:element name="CompensationText" type="nc:TextType" substitutionGroup="nc:ObligationAugmentationPoint"/>
	<xs:element name="EmployerFundedEmployeeBenefitAbstract" abstract="true"/>
	<xs:simpleType name="EmployerFundedEmployeeBenefitCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type for the type of benefits that may be offered to an employee and may be partially/fully funded by the employer.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="disability">
				<xs:annotation>
					<xs:documentation>Disability insurance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="dental">
				<xs:annotation>
					<xs:documentation>Dental insurance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="pension">
				<xs:annotation>
					<xs:documentation>Pension plan</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="medical">
				<xs:annotation>
					<xs:documentation>Medical insurance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="extended medical">
				<xs:annotation>
					<xs:documentation>Extended medical insurance</xs:documentation>
				</xs:annotation>
			</xs:enumeration>	
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="EmployerFundedEmployeeBenefitCodeType">
		<xs:annotation>
			<xs:documentation>A data type for a kind of entity with the obligation.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="epc:EmployerFundedEmployeeBenefitCodeSimpleType">
				<xs:attributeGroup ref="structures:SimpleObjectAttributeGroup"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:element name="EmployerFundedEmployeeBenefitCode" type="epc:EmployerFundedEmployeeBenefitCodeType" substitutionGroup="epc:EmployerFundedEmployeeBenefitAbstract">
		<xs:annotation>
			<xs:documentation>A data type for scaling the base pay amount (in hours) based on the number of hours worked.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EmployerFundedEmployeeBenefitText" type="nc:TextType" substitutionGroup="epc:EmployerFundedEmployeeBenefitAbstract"/>
	<xs:element name="EmployerFundingLevelPercent" type="niem-xs:decimal"/>
	<xs:complexType name="EmployerFundedEmployeeBenefitType">
		<xs:annotation>
			<xs:documentation>A data type for scaling the base pay amount (in hours) based on the number of hours worked.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="structures:ObjectType">
				<xs:sequence>
					<xs:element ref="epc:EmployerFundedEmployeeBenefitAbstract" minOccurs="1" maxOccurs="1"/>
					<xs:element ref="epc:EmployerFundingLevelPercent" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="EmployerFundedEmployeeBenefit" type="epc:EmployerFundedEmployeeBenefitType"/>
	<xs:element name="EmployeeLeaveAbstract" abstract="true"/>
	<xs:element name="EmployeeLeaveDaysPerMonthQuantity" type="nc:QuantityType" substitutionGroup="epc:EmployeeLeaveAbstract">
		<xs:annotation>
			<xs:documentation>Number of days per month that employee will be paid for time working.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EmployeeLeaveDaysPerYearQuantity" type="nc:QuantityType" substitutionGroup="epc:EmployeeLeaveAbstract">
		<xs:annotation>
			<xs:documentation>Number of days per year that employee will be paid for time not working.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EmployeeLeaveCompensationLevelPercent" type="niem-xs:decimal" substitutionGroup="epc:EmployeeLeaveAbstract">
		<xs:annotation>
			<xs:documentation>Additional percentage of wages or salary that the employee will be paid in lieu of time not working.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="EmployerSignature" type="j:SignatureType">
		<xs:annotation>
			<xs:documentation>Name and/or signature of the person who signed the employment offer.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:simpleType name="LabourMarketImpactAssementExemptionCodeSimpleType">
		<xs:annotation>
			<xs:documentation>A data type for the type of labour market impact assessment exemption.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="T11">
				<xs:annotation>
					<xs:documentation>Non-Trade</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T21">
				<xs:annotation>
					<xs:documentation>Trader (Free Trade Agreement (FTA))</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T22">
				<xs:annotation>
					<xs:documentation>Investor (FTA)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="T23">
				<xs:annotation>
